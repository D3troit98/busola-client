<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1"   prefHeight="400.0" prefWidth="600.0" >
    <center>
        <VBox alignment="CENTER" spacing="20.0">
            <Label text="Welcome, Admin" textAlignment="CENTER" fx:id="welcomeLabel" >
                <font>
                    <Font size="20.0" />
                </font>
            </Label>
            <Hyperlink text="View All Members" onAction="#goToViewMembers" />
            <Hyperlink text="View All Items" onAction="#goToViewItems" />
            <Hyperlink text="List of Borrowed Books" onAction="#goToListBorrowedBooks" />
            <Hyperlink text="Logout" onAction="#logout" />
        </VBox>
    </center>
</BorderPane>
